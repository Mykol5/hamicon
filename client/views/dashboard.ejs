<script>
  function addToCart(itemId) {
    // Get the sessionId value from the 'sessionId' cookie
    const sessionId = getCookie('sessionId');

    console.log('addToCart function called with itemId:', itemId);
    // Get the item details from the DOM
    const itemEl = document.getElementById(itemId);
    const itemIdValue = itemEl.getAttribute('data-item-id');
    console.log('Item ID:', itemIdValue);
    const name = itemEl.querySelector('.cart-item-name').textContent;
    console.log('Item name:', name);
    const price = parseFloat(itemEl.querySelector('.cart-item-price').getAttribute('data-price'));
    console.log('Item price:', price);
    const quantity = parseInt(itemEl.querySelector('.cart-item-quantity-value').textContent);
    console.log('Item quantity:', quantity);
    const imageUrl = itemEl.querySelector('.cart-item img').getAttribute('src');
    console.log('Item image URL:', imageUrl);

    // Add the item to the cart only if the quantity is greater than 0
    console.log(quantity)
    if (quantity > 0) {
      // Create the cart item object
      const cartItem = { itemId: itemIdValue, name, price, quantity, imageUrl };

      // Send the cart item data to the server
      fetch('/add-to-cart', {
        method: 'POST',
        headers: {
          'Content-Type': 'application/json',
        },
        body: JSON.stringify(cartItem),
        credentials: 'same-origin', // Include cookies in the request
      })
        .then(response => response.json())
        .then(data => {
          console.log('Item added to cart:', data);
          // Handle the server response if needed
        })
        .catch(error => {
          console.error('Error adding item to cart:', error);
          // Handle the error if needed
        });
    } else {
      console.log('No items added');
    }
  }

  // Helper function to get the value of a cookie by name
  function getCookie(name) {
    const cookieString = document.cookie;
    const cookies = cookieString.split(';');

    for (let i = 0; i < cookies.length; i++) {
      const cookie = cookies[i].trim();

      // Check if the cookie name matches the provided name
      if (cookie.startsWith(name + '=')) {
        // Extract and return the cookie value
        return cookie.substring(name.length + 1);
      }
    }

    // Return null if the cookie is not found
    return null;
  }

  document.addEventListener('DOMContentLoaded', () => {
    // Your code here
  });
</script>


