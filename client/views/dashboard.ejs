@keyframes news-flash-animation {
  0% {
    /* Initial state of the news flash */
    transform: translateX(100%);
  }
  50% {
    /* Halfway point of the animation */
    transform: translateX(0%);
  }
  100% {
    /* Final state of the news flash */
    transform: translateX(-100%);
  }
}



.news-flash {
  /* Other CSS styles */
  animation-name: news-flash-animation;
  animation-duration: 15s;
  animation-timing-function: linear; /* Adjust the timing function as needed */
